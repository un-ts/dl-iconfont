{
  "name": "dl-iconfont",
  "version": "0.4.2",
  "type": "module",
  "description": "An iconfont downloader via puppeteer.",
  "repository": "https://github.com/un-ts/dl-iconfont.git",
  "author": "Zhenggang Su (https://github.com/Edisonsu768) <zgsu@alauda.io>",
  "contributors": [
    "JounQin (https://www.1stG.me) <admin@1stg.me>"
  ],
  "funding": "https://opencollective.com/unts",
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "bin": {
    "dli": "./lib/cli.js",
    "dl-iconfont": "./lib/cli.js",
    "iconfont-dl": "./lib/cli.js"
  },
  "main": "lib/index.cjs",
  "module": "lib",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "lib",
  "files": [
    "lib"
  ],
  "keywords": [
    "download-iconfont",
    "dl-iconfont",
    "iconfont",
    "iconfont-dl",
    "iconfont-downloader"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -f cjs",
    "build:tsc": "tsc -b",
    "dev": "tsx ./src/cli",
    "lint": "run-p lint:*",
    "lint:es": "eslint . -f friendly",
    "lint:tsc": "tsc --noEmit",
    "prepare": "simple-git-hooks && yarn-deduplicate --strategy fewer || exit 0",
    "prerelease": "yarn build",
    "release": "changeset publish",
    "typecov": "type-coverage"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.0.0",
    "tslib": "^2.6.2",
    "x-fetch": "^0.2.0"
  },
  "devDependencies": {
    "@1stg/lib-config": "^13.0.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/web": "^0.0.214",
    "size-limit": "^11.0.1",
    "size-limit-preset-node-lib": "^0.3.0",
    "tsx": "^4.6.2",
    "type-coverage": "^2.27.0",
    "typescript": "^5.3.3",
    "yarn-deduplicate": "^6.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "lib/cli.js",
      "limit": "750B"
    }
  ],
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreNested": true,
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
